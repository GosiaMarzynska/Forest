<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Kontakt | Forest</title>
    <meta name="description" content="Skontaktuj się z nami by przeżyć niezapomnianą przygodę. Odetchnij od codziennych obowiązków">
    <script src="https://kit.fontawesome.com/2755310439.js" crossorigin="anonymous"></script>
    <!--@include "links.kit"-->
</head>

<body>

    <!--@include "nav.kit"-->

    <header class="header section color-section">
        <div class="header__text">
            <p class="header__text-label">Forest Group</p>
            <h1 class="header__text-heading">Najpiękniejszy park w Twojej okolicy</h1>
        </div>
    </header>

    <section class="section info-section">
        <h2 class="section-heading">KONTAKT</h2>
        <div class="wrapper">
            <div class="info">
                <ul class="info__list">
                    <li class="info__list-item">Forest Group Company Ltd.</li>
                    <li class="info__list-item">ul. Jasogórska 2</li>
                    <li class="info__list-item"> 31-358 Kraków</li>
                    <li class="info__list-item">✉️ <a href="#" class="info__list-item-link">contact@forest.com</a>
                    </li>
                </ul>
            </div>
        </div>
    </section>

    <section class="map-section">
        <h2 class="section-heading">MAPA</h2>
        <div class="wrapper">
            <div class="map">
                <div class="mapouter">
                    <div class="gmap_canvas"><iframe width="600" height="500" id="gmap_canvas"
                            src="https://maps.google.com/maps?q=Krak%C3%B3w,%20ul.%20Jasnog%C3%B3rska%202&t=k&z=17&ie=UTF8&iwloc=&output=embed"
                            frameborder="0" scrolling="no" marginheight="0" marginwidth="0">
                        </iframe>
                        <a href="https://2piratebay.org"></a>
                        <br>
                        <a href="https://www.embedgooglemap.net">embed google</a>
                    </div>
                </div>
            </div>
        </div>
    </section>


    <section class="contact">
    <h2 class="section-heading">Napisz do nas!</h2>
    <form class="contact__form">
        <div class="contact__form-box">
            <label for="name">Your name:</label>
            <input type="text" id="name">
        </div>
        <div class="contact__form-box">
            <label for="mail">Your e-mail:</label>
            <input type="text" id="mail">
        </div>
        <div class="contact__form-box">
            <label for="msg">Message:</label>
            <textarea id="msg" cols="30" rows="10"></textarea>
        </div>

        <button type="submit" class="contact__form-btn">Send</button>

    </form>

</section>



    <!--@include "footer.kit"-->
    <script src="dist/js/main.min.js"></script>
</body>

</html>